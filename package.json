    "create-hash": "^1.2.0",
    "eslint": "5.16.0",
    "eslint-config-webbylab": "4.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-more": "0.3.1",
    "eslint-plugin-no-require-lodash": "1.1.0",
